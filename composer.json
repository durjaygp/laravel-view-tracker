{
    "name": "durjaygp/laravel-view-tracker",
    "version": "1.0.0",
    "description": "A lightweight view tracker for Eloquent models in Laravel.",
    "type": "library",
    "require": {
      "php": "^8.0",
      "illuminate/support": "^8.0|^9.0|^10.0|^11.0"
    },
    "autoload": {
      "psr-4": {
        "Durjaygp\\ViewTracker\\": "src/"
      }
    },
    "authors": [
        {
            "name": "Durjay Ghosh",
            "email": "42237029+durjaygp@users.noreply.github.com"
        }
    ],
    "repositories": [
  {
    "type": "vcs",
    "url": "https://github.com/durjaygp/laravel-view-tracker"
  }     
  ],
    "extra": {
      "laravel": {
        "providers": [
          "Durjaygp\\ViewTracker\\ViewTrackerServiceProvider"
        ],
        "aliases": {
          "ViewTracker": "Durjaygp\\ViewTracker\\Facades\\ViewTracker"
        }
      }
    },
    "license": "MIT",
    "minimum-stability": "dev"
  }
  